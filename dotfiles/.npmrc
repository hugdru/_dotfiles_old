;;;;
; npm userconfig file
; this is a simple ini-formatted file
; lines that start with semi-colons are comments.
; read `npm help config` for help on the various options
;;;;

prefix=${HOME}/.npm-packages
strict-ssl=true

;;;;
; all options with default values
;;;;
; access=null
; always-auth=false
; also=null
; bin-links=true
; browser=null
; ca=null
; cafile=undefined
; cache=/home/hugdru/.npm
; cache-lock-stale=60000
; cache-lock-retries=10
; cache-lock-wait=10000
; cache-max=null
; cache-min=10
; cert=null
; color=true
; depth=null
; description=true
; dev=false
; dry-run=false
; editor=vim
; engine-strict=false
; force=false
; fetch-retries=2
; fetch-retry-factor=10
; fetch-retry-mintimeout=10000
; fetch-retry-maxtimeout=60000
; git=git
; git-tag-version=true
; global=false
; globalconfig=/home/hugdru/.npm-packages/etc/npmrc
; global-style=false
; group=1000
; heading=npm
; if-present=false
; ignore-scripts=false
; init-module=/home/hugdru/.npm-init.js
; init-author-name=
; init-author-email=
; init-author-url=
; init-version=1.0.0
; init-license=ISC
; json=false
; key=null
; legacy-bundling=false
; link=false
; local-address=undefined
; loglevel=warn
; long=false
; message=%s
; node-version=5.7.0
; npat=false
; onload-script=null
; only=null
; optional=true
; parseable=false
; prefix=/usr
; production=false
; progress=true
; proprietary-attribs=true
; proxy=null
; https-proxy=null
; user-agent=npm/{npm-version} node/{node-version} {platform} {arch}
; rebuild-bundle=true
; registry=https://registry.npmjs.org/
; rollback=true
; save=false
; save-bundle=false
; save-dev=false
; save-exact=false
; save-optional=false
; save-prefix=^
; scope=
; searchopts=
; searchexclude=null
; searchsort=name
; shell=/bin/zsh
; shrinkwrap=true
; sign-git-tag=false
; strict-ssl=true
; tag=latest
; tag-version-prefix=v
; tmp=/tmp/hugdru
; unicode=true
; unsafe-perm=true
; usage=false
; user=1000
; userconfig=/home/hugdru/.npmrc
; umask=18
; version=false
; versions=false
; viewer=man
; _exit=true
; globalignorefile=/home/hugdru/.npm-packages/etc/npmignore
